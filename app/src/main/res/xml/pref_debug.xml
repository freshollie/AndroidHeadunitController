<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <com.freshollie.headunitcontroller.utils.DummyPreference/>
    <SwitchPreference
        android:defaultValue="false"
        android:key="@string/pref_debug_enabled_key"
        android:summaryOn="@string/pref_debug_toggle_enabled_summary"
        android:summaryOff="@string/pref_debug_toggle_disabled_summary"
        android:title="@string/pref_debug_toggle_title"/>

    <SwitchPreference
        android:defaultValue="false"
        android:enabled="false"
        android:key="@string/pref_power_on_debug_key"
        android:summary="@string/pref_debug_power_on_toggle_summary"
        android:title="@string/pref_debug_power_on_toggle_title"/>
</PreferenceScreen>
